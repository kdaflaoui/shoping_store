
@{
    ViewBag.Title = "Index";
    Layout = null;

}


<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="initial-scale=1.0, user-scalable=no">
    <meta charset="utf-8">
    <title>Directions Service</title>
    <style>
        #map {
            width: 500px;
            height: 500px;
        }
    </style>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
    <script async defer src="https://maps.googleapis.com/maps/api/js?v=3.exp&sensor=false&callback=initialize"></script>
</head>
<body>
    <div id="map">

    </div>

    <script>

        function initialize() {
          var directionsService = new google.maps.DirectionsService();
          var directionsDisplay = new google.maps.DirectionsRenderer();
        var quebec = new google.maps.LatLng(52.939915, -73.549133);
        var montreal = new google.maps.LatLng(45.509060, -73.553360);
            var mapOptions = {
                zoom: 7,
                mapTypeId: google.maps.MapTypeId.ROADMAP,
                center: montreal
            }

          var mapdire = new google.maps.Map(document.getElementById("map"), mapOptions);
          directionsDisplay.setMap(mapdire);
        }


    </script>


</body>
</html>

